@model IEnumerable<ProyectoInventarioASP.Models.MicroProcesador>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-primary btn-sm">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Numero de serie</th>
            <th scope="col">Marca</th>
            <th scope="col">Tecnologia</th>
            <th scope="col">Estado</th>
            <th>
                Opciones
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.NumSerieId</td>
                <td>@obj.Marca</td>
                <td>@obj.Tecnologia</td>
                <td>@obj.estado</td>
                <td>
                    <a asp-action="Edit" class="btn btn-primary btn-sm" asp-route-id="@obj.NumSerieId">Edit</a> |
                    <a asp-action="Details" class="btn btn-primary btn-sm" asp-route-id="@obj.NumSerieId">Details</a> |
                    <a asp-action="Delete" class="btn btn-danger btn-sm" asp-route-id="@obj.NumSerieId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
