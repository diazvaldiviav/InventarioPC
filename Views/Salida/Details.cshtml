@model ProyectoInventarioASP.Models.Salida

@{
    ViewData["Title"] = "Details";
    Layout = "_Layout";
}

<h1>Detalles de la salida</h1>

<div>
    <h4>Salida No: @Html.DisplayFor(model => model.Id)</h4>
    <hr />
   <div class="container">
        <div class="row">
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.salida)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.salida)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.FechaSalida)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.FechaSalida)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.observaciones)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.observaciones)</div>
        </div>
</div>
<div class="container-flex">
        <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary btn-sm">Modificar</a>
        <a asp-action="Index">Volver</a>
    </div>

