@model Display

@{
    ViewData["Title"] = "Index";
    Layout = null;
}
<!DOCTYPE html>

<html>

<head>
    <meta name="viewport" content="width=device-width" />
    <title>ContactPDF</title>
    <link rel="shortcut icon" href="~/Assets/logo.png" type="image/x-icon">
    <!-- Custom styles -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

</head>

<body>

    <div>
        <h4>Display No: @Html.DisplayFor(model => model.Id)</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NumInv)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NumInv)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NumSerie)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NumSerie)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Marca)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Marca)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.NumInvPc)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NumInvPc)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.estado)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.estado)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Computadora)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Computadora.Id)
            </dd>
        </dl>
    </div>
</body>

</html>