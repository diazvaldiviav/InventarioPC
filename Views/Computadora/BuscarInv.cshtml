@model Computadora
@{
    ViewData["Title"] = "Computadora";
}

<h2>Borrar Pc</h2>

<h3>Vas a buscar una Pc</h3>
<div>
    <h4>Ingresa el inv de la PC</h4>
    <form method="post">
