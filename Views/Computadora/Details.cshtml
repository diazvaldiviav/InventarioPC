            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.MotherBoardMarca)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.MotherBoardMarca)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.MemoriaRamCap)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.MemoriaRamCap)</div>
