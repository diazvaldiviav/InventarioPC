            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.UpsInv)</div>
        </div>
    </div>
    <div class="container-flex">
        <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary btn-sm">Modificar</a>
        <a asp-action="Index">Volver</a>
    </div>
</div>