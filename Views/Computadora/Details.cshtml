@model ProyectoInventarioASP.Models.Computadora

@{
    ViewData["Title"] = "Details";
    Layout = "_Layout";
}
<div class="margin">
    <h1>Detalles</h1>
    <h3>Computadora No: @Html.DisplayFor(model => model.Id)</h3>
    <div class="container">
        <div class="row">
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.NumInv)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.NumInv)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.Nombre)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.Nombre)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.NombreDepartamento)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.NombreDepartamento)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.NombreArea)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.NombreArea)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.NumIp)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.NumIp)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.Mac)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.Mac)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.UserName)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.UserName)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.DiscoDuroCap)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.DiscoDuroCap)</div>
            <div class="w-100"></div>
            <div class="col p-2 bg-light border">@Html.DisplayNameFor(model => model.DiscoDuroTipoCon)</div>
            <div class="col p-2 bg-light border">@Html.DisplayFor(model => model.DiscoDuroTipoCon)</div>
            <div class="w-100"></div>
