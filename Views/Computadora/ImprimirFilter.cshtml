@model ProyectoInventarioASP.Models.Computadora

@{
    ViewData["Title"] = "Create";
    Layout = "_Layout";
}
 
    <h1>Imprimir</h1>

    <h4>Ingrese un dato de las pc que quieres imprimir</h4>
    <hr />

    <form method="post" asp-controller="computadora", asp-action="Imprimir">
           <div class="form-row col-md-5 border border-1">
            <div class="form-group">
                <input asp-for="NumInv" class="form-control border border-5" required />
            </div>

            <div class="form-group p-2">
                <input type="submit" value="Buscar" class="btn btn-orange" />
            </div>
        </div>
    </form>

    <div>
        <a asp-action="Index">Volver</a>
    </div>

    @section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }

